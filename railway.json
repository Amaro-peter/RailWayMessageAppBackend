{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "chmod +x ./apirestfulv1/mvnw && ./apirestfulv1/mvnw -f ./apirestfulv1/pom.xml clean install -DskipTests"
  },
  "deploy": {
    "startCommand": "java -jar ./apirestfulv1/target/apirestfulv1-0.0.1-SNAPSHOT.jar",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}